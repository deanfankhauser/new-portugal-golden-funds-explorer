
{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "cleanUrls": false,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "/sitemap.xml"
    },
    {
      "source": "/robots.txt",
      "destination": "/robots.txt"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://funds.movingto.com/$1",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "www.funds.movingto.com"
        }
      ]
    },
    {
      "source": "/(.*)",
      "destination": "https://funds.movingto.com/$1",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "funds.movingto.io"
        }
      ]
    },
    {
      "source": "/funds",
      "destination": "/index",
      "permanent": true
    },
    {
      "source": "/funds/",
      "destination": "/index",
      "permanent": true
    },
    {
      "source": "/:num(\\d+)-return",
      "destination": "/tags/:num-percent-return",
      "permanent": true
    },
    {
      "source": "/:num(\\d+)-annual-yield",
      "destination": "/tags/:num-percent-yield",
      "permanent": true
    },
    {
      "source": "/:num(\\d+)-dividend",
      "destination": "/tags/:num-percent-dividend",
      "permanent": true
    },
    {
      "source": "/small-cap-50m",
      "destination": "/tags/small-cap-less-than-50m",
      "permanent": true
    },
    {
      "source": "/mid-cap",
      "destination": "/tags/mid-cap-50-100m",
      "permanent": true
    },
    {
      "source": "/large-cap",
      "destination": "/tags/large-cap-100m-plus",
      "permanent": true
    },
    {
      "source": "/:slug(balanced|clean-energy-solar-battery-storage|debt|fixed-income-digital-assets|infrastructure|mixed|multi-asset|private-debt-hybrid-instruments|private-equity|private-equity-debt|private-equity-venture-capital|real-estate|venture-capital)",
      "destination": "/categories/:slug",
      "permanent": true
    },
    {
      "source": "/:slug(balanced|clean-energy-solar-battery-storage|debt|fixed-income-digital-assets|infrastructure|mixed|multi-asset|private-debt-hybrid-instruments|private-equity|private-equity-debt|private-equity-venture-capital|real-estate|venture-capital)/",
      "destination": "/categories/:slug",
      "permanent": true
    },
    {
      "source": "/:slug(3-5-percent-annual-yield|5-10-year-lock-up|5-percent-dividend|5-percent-yield|12-percent-return|1-1-5-percent-management-fee|250k-350k-subscription-min-only-gv-still-requires-500k-total|300k-400k-subscription-min-only-gv-still-requires-500k-total|350k-500k-subscription-min-only-gv-still-requires-500k-total|400k-600k-subscription-min-only-gv-still-requires-500k-total|500k-plus|ai-driven|balanced|battery-storage|bitcoin|bonds|capital-growth|capital-preservation|circular-economy|climate|closed-ended|crypto|daily-nav|debt|deposits|diversified|dividends|energy|energy-as-a-service|equities|equity|ethereum|fund-subscription-minimums|gold|golden-visa-eligible|golden-visa-funds-for-australian-citizens|golden-visa-funds-for-canadian-citizens|golden-visa-funds-for-chinese-citizens)",
      "destination": "/tags/:slug",
      "permanent": true
    },
    {
      "source": "/:slug(3-5-percent-annual-yield|5-10-year-lock-up|5-percent-dividend|5-percent-yield|12-percent-return|1-1-5-percent-management-fee|250k-350k-subscription-min-only-gv-still-requires-500k-total|300k-400k-subscription-min-only-gv-still-requires-500k-total|350k-500k-subscription-min-only-gv-still-requires-500k-total|400k-600k-subscription-min-only-gv-still-requires-500k-total|500k-plus|ai-driven|balanced|battery-storage|bitcoin|bonds|capital-growth|capital-preservation|circular-economy|climate|closed-ended|crypto|daily-nav|debt|deposits|diversified|dividends|energy|energy-as-a-service|equities|equity|ethereum|fund-subscription-minimums|gold|golden-visa-eligible|golden-visa-funds-for-australian-citizens|golden-visa-funds-for-canadian-citizens|golden-visa-funds-for-chinese-citizens)/",
      "destination": "/tags/:slug",
      "permanent": true
    },
    {
      "source": "/:slug(golden-visa-funds-for-u-s-citizens|golden-visa-funds-for-uk-citizens|greater-than-1-5-percent-management-fee|greater-than-10-year-lock-up|greater-than-5-annual-yield|greater-than-5-percent-annual-yield|healthcare|high-risk|hybrid|industrial|infrastructure|large-cap-greater-than-100m|less-than-1-percent-management-fee|less-than-3-percent-annual-yield|less-than-5-year-lock-up|liquid|lock-up|long-term|low-risk|medium-risk|mid-cap|mid-cap-50-100m|no-fees|no-lock-up|open-ended|pfic-compliant|portugal|private-equity|qef-eligible|real-estate|regulated|renewable-energy|secondary-market|small-cap-less-than-50m|smes|solar|solana|special-situations)",
      "destination": "/tags/:slug",
      "permanent": true
    },
    {
      "source": "/:slug(golden-visa-funds-for-u-s-citizens|golden-visa-funds-for-uk-citizens|greater-than-1-5-percent-management-fee|greater-than-10-year-lock-up|greater-than-5-annual-yield|greater-than-5-percent-annual-yield|healthcare|high-risk|hybrid|industrial|infrastructure|large-cap-greater-than-100m|less-than-1-percent-management-fee|less-than-3-percent-annual-yield|less-than-5-year-lock-up|liquid|lock-up|long-term|low-risk|medium-risk|mid-cap|mid-cap-50-100m|no-fees|no-lock-up|open-ended|pfic-compliant|portugal|private-equity|qef-eligible|real-estate|regulated|renewable-energy|secondary-market|small-cap-less-than-50m|smes|solar|solana|special-situations)/",
      "destination": "/tags/:slug",
      "permanent": true
    },
    {
      "source": "/:slug(sustainability|tax-free|technology|tourism|ucits|venture-capital)",
      "destination": "/tags/:slug",
      "permanent": true
    },
    {
      "source": "/:slug(sustainability|tax-free|technology|tourism|ucits|venture-capital)/",
      "destination": "/tags/:slug",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'self' 'unsafe-inline' https://www.googletagmanager.com https://static.cloudflareinsights.com https://challenges.cloudflare.com https://server.fillout.com https://cdn.gpteng.co; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com https://fonts.gstatic.com; img-src 'self' data: https:; font-src 'self' https://fonts.gstatic.com; connect-src 'self' https: wss: https://www.google-analytics.com https://analytics.google.com; frame-src https://server.fillout.com https://form.fillout.com; frame-ancestors 'none';"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains; preload"
        }
      ]
    },
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, s-maxage=86400"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/lovable-uploads/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/:file*.:ext(js|css|woff|woff2|ttf|eot)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/:file*.:ext(png|jpg|jpeg|gif|ico|svg|webp)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, s-maxage=3600, stale-while-revalidate=86400"
        }
      ]
    }
  ]
}
